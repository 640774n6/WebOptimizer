<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <_WebOptimizerCacheDirectory>$(MSBuildProjectDirectory)\obj\WebOptimizerCache</_WebOptimizerCacheDirectory>
  </PropertyGroup>

  <Target Name="CleanWebOptimizerCache" AfterTargets="Clean">
    <RemoveDir Directories="$(_WebOptimizerCacheDirectory)" />
  </Target>

</Project>
